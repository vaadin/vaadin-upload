{
  "preset": "google",
  "maximumLineLength": 140,
  "disallowMultipleVarDecl": false,
  "validateIndentation": false,
  "disallowTrailingWhitespace": false,
  "jsDoc": {
    "checkAnnotations": {
      "extra": {
        "polymerBehavior": true,
        "param": true,
        "return": true,
        "private": true,
        "default": true,
        "event": true,
        "property": true,
        "type": true,
        "method": true
      }
    }
  }
}
